datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  posts Post[]
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  published Boolean @default(false)
  author    User    @relation(fields: [authorId], references: [id])
  authorId  Int
}

model PokemonType {
  id   Int    @id @default(autoincrement())
  name String
}

model Pokemon {
  id           Int    @id @default(autoincrement())
  name         String
  typeId       Int
  height       String
  weight       String
  candy        String
  candy_count  String
  spawn_chance Int
  avg_spawns   Int
  spawn_time   String
}
