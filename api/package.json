{
  "name": "api",
  "private": true,
  "version": "0.0.0",
  "module": "nodenext",
  "scripts": {
    "start": "ts-node ./index.ts",
    "dev": "concurrently -p \"{name}\" -c \"bgGreen,bgBlue\" --n \"API,Data\" \"nodemon ./index.ts\" \"prisma studio\"",
    "studio": "prisma studio --browser none",
    "migrate:dev": "prisma migrate dev"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.0",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "express": "^4.18.1"
  }
}
